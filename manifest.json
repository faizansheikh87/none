{
    "manifest_version": 3,
    "name": "LinkedIn Auto Connect",
    "description": "Automatically connect with people on LinkedIn",
    "version": "1.0",
    "permissions": [
      "activeTab",
      "scripting"
    ],
    "content_scripts": [
      {
        "matches": ["https://www.linkedin.com/mynetwork/grow/*"],
        "js": ["contentScript.js"]
      }
    ],
    "action": {
      "default_popup": "popup.html"
    }
  }
  